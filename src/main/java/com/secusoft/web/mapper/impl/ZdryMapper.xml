<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.secusoft.web.mapper.ZdryMapper">

    <insert id="insertBatchQgzt" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO vi_view_qgzt (pic_id,human_name,pic,update_time,status) values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.picId}, #{item.humanName}, #{item.pic}, #{item.updateTime}, #{item.status})
        </foreach>
    </insert>

    <insert id="insertBatchSgy" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO vi_view_sgy (pic_id,human_name,pic,update_time,status) values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.picId}, #{item.humanName}, #{item.pic}, #{item.updateTime}, #{item.status})
        </foreach>
    </insert>

    <insert id="insertBatchSdts" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO vi_view_sdts (pic_id,human_name,pic,update_time,status) values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.picId}, #{item.humanName}, #{item.pic}, #{item.updateTime}, #{item.status})
        </foreach>
    </insert>

    <insert id="insertQgzt" parameterType="com.secusoft.web.model.ZdryBean" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO vi_view_qgzt (pic_id,human_name,pic,update_time,status) values
            (#{picId}, #{humanName}, #{pic}, #{updateTime}, #{status})
    </insert>

    <insert id="insertSgy" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO vi_view_sgy (pic_id,human_name,pic,update_time,status) values
            (#{picId}, #{humanName}, #{pic}, #{updateTime}, #{status})
    </insert>

    <insert id="insertSdts" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO vi_view_sdts (pic_id,human_name,pic,update_time,status) values
            (#{picId}, #{humanName}, #{pic}, #{updateTime}, #{status})
    </insert>
</mapper>