<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.secusoft.web.mapper.ViRepoMapper">

    <insert id="insertBatchQgzt" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO view_qgzt (pic_id,human_name,pic,update_time,status) values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.picId}, #{item.humanName}, #{item.pic}, #{item.updateTime}, #{item.status})
        </foreach>
    </insert>

